<p id="notice"><%= notice %></p>

<h1>Billing Informations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address line 1</th>
      <th>Address line 2</th>
      <th>City</th>
      <th>State</th>
      <th>Zip code</th>
      <th>Phone number</th>
      <th>Cc type</th>
      <th>Cc last four</th>
      <th>User</th>
      <th>Invoice</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @billing_informations.each do |billing_information| %>
      <tr>
        <td><%= billing_information.name %></td>
        <td><%= billing_information.address_line_1 %></td>
        <td><%= billing_information.address_line_2 %></td>
        <td><%= billing_information.city %></td>
        <td><%= billing_information.state %></td>
        <td><%= billing_information.zip_code %></td>
        <td><%= billing_information.phone_number %></td>
        <td><%= billing_information.cc_type %></td>
        <td><%= billing_information.cc_last_four %></td>
        <td><%= billing_information.user %></td>
        <td><%= billing_information.invoice %></td>
        <td><%= link_to 'Show', billing_information %></td>
        <td><%= link_to 'Edit', edit_billing_information_path(billing_information) %></td>
        <td><%= link_to 'Destroy', billing_information, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Billing Information', new_billing_information_path %>
